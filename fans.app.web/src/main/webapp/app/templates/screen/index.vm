#foreach($each in $list)
<div class="am-g doc-am-g" style="padding:5px;">
	<div class="am-u-sm-3">
		<img src="$!each.headImg" class="am-img-thumbnail am-circle"/>
	</div>
	<div class="am-u-sm-6" style="padding-left:5px">
		<p>
			<font color="grey">[浙江 杭州]</font> $!each.nickName
			<br>
			$!each.description
		</p>
	</div>
	<div class="am-u-sm-3">
		<button type="button" class="am-btn am-btn-success am-btn-sm" style="margin-top:25px;" onclick="qrcodeShow('$!each.qrcode')">加为好友</button>
	</div>
</div>
#end